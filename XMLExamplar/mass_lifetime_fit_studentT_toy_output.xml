            <Value>0.045</Value>
            <Type>Fixed</Type>
            <Unit>unitless</Unit>
        </PhysicsParameter>
        <PhysicsParameter>
            <Name>timeResolution2</Name>
            <Value>0.0</Value>
            <Type>Fixed</Type>
            <Unit>unitless</Unit>
        </PhysicsParameter>
        <PhysicsParameter>
            <Name>timeResolution3</Name>
            <Value>0.0</Value>
            <Type>Fixed</Type>
            <Unit>unitless</Unit>
        </PhysicsParameter>
        <PhysicsParameter>
            <Name>timeOffset</Name>
            <Value>0.0</Value>
