        </PhysicsParameter>
        <PhysicsParameter>
            <Name>timeResolutionScale2</Name>
            <Value>0.0</Value>
            <Type>Fixed</Type>
            <Unit>unitless</Unit>
        </PhysicsParameter>
        <PhysicsParameter>
            <Name>timeResolutionScale3</Name>
            <Value>0.0</Value>
            <Type>Fixed</Type>
            <Unit>unitless</Unit>
        </PhysicsParameter>
        <PhysicsParameter>
            <Name>alphaM_pr</Name>
            <Value>0.0016</Value>
            <Minimum>-0.01</Minimum>
            <Maximum>0.01</Maximum>
            <Type>Free</Type>
            <Unit>Unitless</Unit>
        </PhysicsParameter>
        <PhysicsParameter>
            <Name>fractionLL1</Name>
            <Value>0.9921</Value>
            <Minimum>0.8</Minimum>
            <Maximum>1.0</Maximum>
            <Type>Free</Type>
            <Unit>Unitless</Unit>
        </PhysicsParameter>
        <PhysicsParameter>
            <Name>tauLL1</Name>
            <Value>0.177</Value>
            <Minimum>0.01</Minimum>
            <Maximum>1.5</Maximum>
            <Type>Free</Type>
            <Unit>Unitless</Unit>
        </PhysicsParameter>
        <PhysicsParameter>
            <Name>tauLL2</Name>
            <Value>1.41</Value>
            <Minimum>0.9</Minimum>
            <Maximum>2.8</Maximum>
            <Type>Free</Type>
            <Unit>Unitless</Unit>
        </PhysicsParameter>
    </ParameterSet>
    <Minimiser>
        <MinimiserName>Minuit</MinimiserName>
        <MaxSteps>100000</MaxSteps>
        <GradTolerance>1.0E-4</GradTolerance>
        <Quality>1</Quality>
    </Minimiser>
    <FitFunction>
        <FunctionName>NegativeLogLikelihoodThreaded</FunctionName>
        <Threads>8</Threads>
        <SetIntegratorTest>True</SetIntegratorTest>
    </FitFunction>
    <NumberRepeats>1</NumberRepeats>
    <CommonPhaseSpace>
        <PhaseSpaceBoundary/>
    </CommonPhaseSpace>
    <CommonPDF>
        <PDF>
            <Name>null</Name>
        </PDF>
    </CommonPDF>
    <Precalculator/>
    <ToFit>
        <NormalisedSumPDF>
            <FractionName>SignalFraction</FractionName>
            <ProdPDF>
                <PDF>
                    <Name>Exponential</Name>
                    <ConfigurationParameter>UseNumericalIntegration:False</ConfigurationParameter>
                    <ConfigurationParameter>UseTimeAcceptance:False</ConfigurationParameter>
                    <ConfigurationParameter>UseEventResolution:False</ConfigurationParameter>
                </PDF>
                <PDF>
                    <Name>StudentT</Name>
                </PDF>
            </ProdPDF>
            <ProdPDF>
                <PDF>
                    <Name>DoubleExponential</Name>
                    <ConfigurationParameter>UseNumericalIntegration:False</ConfigurationParameter>
                    <ConfigurationParameter>UseTimeAcceptance:False</ConfigurationParameter>
                    <ConfigurationParameter>UseEventResolution:False</ConfigurationParameter>
                </PDF>
                <PDF>
                    <Name>Bs2JpsiPhiMassBkg</Name>
                    <ConfigurationParameter>UseNumericalIntegration:False</ConfigurationParameter>
                </PDF>
            </ProdPDF>
        </NormalisedSumPDF>
        <DataSet>
            <Source>Foam</Source>
            <NumberEvents>4000</NumberEvents>
            <PhaseSpaceBoundary>
                <Observable>
                    <Name>mass</Name>
                    <Minimum>5300.0</Minimum>
                    <Maximum>5450.0</Maximum>
                    <Unit>MeV/c^{2}</Unit>
                </Observable>
                <Observable>
                    <Name>time</Name>
                    <Minimum>0.3</Minimum>
                    <Maximum>14.0</Maximum>
                    <Unit>ps</Unit>
                </Observable>
            </PhaseSpaceBoundary>
        </DataSet>
    </ToFit>
    <ToFit>
        <ConstraintFunction/>
    </ToFit>
    <Output>
        <ComponentProjection>
            <DataBins>50</DataBins>
            <LogY>False</LogY>
            <Name>mass</Name>
            <WidthKey>3:3:3</WidthKey>
            <ColorKey>1:16:12</ColorKey>
            <StyleKey>1:1:4</StyleKey>
            <CompNames>Total:Signal:Background</CompNames>
            <XTitle>m(J/#psi #eta) [MeV/#it{c}^{2}]</XTitle>
            <DrawPull>True</DrawPull>
            <AddLHCb>True</AddLHCb>
        </ComponentProjection>
        <ComponentProjection>
            <LogY>True</LogY>
            <Name>time</Name>
            <WidthKey>3:3:3</WidthKey>
            <ColorKey>1:16:12</ColorKey>
            <StyleKey>1:1:4</StyleKey>
            <CompNames>Total:Signal:Background</CompNames>
            <XTitle>Decay time [ps]</XTitle>
            <DrawPull>True</DrawPull>
            <AddLHCb>True</AddLHCb>
        </ComponentProjection>
    </Output>
</RapidFit>
