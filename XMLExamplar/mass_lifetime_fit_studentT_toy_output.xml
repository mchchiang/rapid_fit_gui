        <PhysicsParameter>
            <Name>s</Name>
            <Value>7.2</Value>
            <Minimum>4.0</Minimum>
            <Maximum>10.0</Maximum>
            <Type>Free</Type>
            <Unit>MeV/c^{2}</Unit>
        </PhysicsParameter>
        <PhysicsParameter>
            <Name>n</Name>
            <Value>14.4</Value>
            <Minimum>-10.1</Minimum>
            <Maximum>100.0</Maximum>
            <Type>Free</Type>
            <Unit>MeV/c^{2}</Unit>
        </PhysicsParameter>
        <PhysicsParameter>
